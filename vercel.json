{
  "headers": [
    {
      "source": "/boot.sh",
      "headers": [
        { "key": "Content-Type", "value": "text/x-shellscript; charset=utf-8" },
        { "key": "Cache-Control", "value": "no-cache" }
      ]
    }
  ],
  "redirects": [
    { "source": "/", "destination": "/boot.sh", "permanent": false }
  ],
  "cleanUrls": false
}
